	Doi = {10.1086/165412},
	Journal = {\apj},
	Keywords = {BINARY STARS, MASS TRANSFER, POLYTROPIC PROCESSES, STELLAR MASS EJECTION, STELLAR MODELS, ADIABATIC CONDITIONS, MAIN SEQUENCE STARS, STELLAR ENVELOPES, STELLAR EVOLUTION, STELLAR MASS ACCRETION, TABLES (DATA)},
	Month = jul,
