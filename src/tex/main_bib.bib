	Journal = {Living Reviews in Relativity},
	Keywords = {AM CVn stars, Neutron stars, Astrophysics, Gravitational-wave sources, Black holes, Binary systems, White dwarfs, Supernovae, Astrophysics - High Energy Astrophysical Phenomena, Astrophysics - Solar and Stellar Astrophysics, General Relativity and Quantum Cosmology},
	Month = {May},
