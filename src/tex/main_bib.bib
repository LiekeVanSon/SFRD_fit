	Eid = {L48},
	Eprint = {1108.2752},
	Journal = {\apjl},
	Keywords = {binaries: close, stars: evolution, stars: mass-loss},
	Month = oct,
	Pages = {L48},
	Primaryclass = {astro-ph.SR},
	Title = {{Can We Trust Models for Adiabatic Mass Loss?}},
	Volume = 739,
